{
  "manifest_version": 2,
  "name": "Todo Chrome Extension",
  "version": "1.0",
  "permissions": [
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" 
  },
  "description": "A todo app you can add, delete and drag item"
}